
<%= form_for [@artist, @song] do |f| %>

<div class="panel panel-primary">
    <div class="panel-body">
      <div class="form-group">
        <label>Name</label>
        <%= f.text_field :name, placeholder: "Songtitle", class: "form-control" %>
      </div>
      <div class="actions">
        <%= f.submit "Save", class: "btn btn-primary" %>
      </div>
    </div>
</div>

<% end %>
